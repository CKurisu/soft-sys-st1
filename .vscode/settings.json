{
    "fileHeaderComment.parameter": {
        "*": {
            "commentbegin": "/*",
            "commentprefix": " *",
            "commentend": "*/",
            "commentbeginht": "<!--",
            "commentprefixht": " --",
            "commentendht": "-->",
            "name": "Develop Site",
            "header": "Developed with Bootstrap",
            "autor": "@author SoftSystem, LTD.",
            "version": "@version   v1.0.1",
            "copy": "@copyright    (c) 2013 - SoftSystem, LTD. All rights reserved.",
            "licence": "@licence   Licensed under the MIT license."
        }
    },
    "fileHeaderComment.template": {
        "*": [
            "${commentbegin}",
            "${commentprefix} ${name}",
            "${commentprefix} ${header}",
            "${commentprefix}",
            "${commentprefix} ${version}, Created on ${date} ${time}",
            "${commentprefix} ${autor}",
            "${commentprefix} ${copy}",
            "${commentprefix} ${licence}",
            "${commentend}"
        ],
        "html": [
            "${commentbeginht}",
            "${commentprefixht} ${name}",
            "${commentprefixht} ${header}",
            "${commentprefixht}",
            "${commentprefixht} ${version}, Created on ${date} ${time}",
            "${commentprefixht} ${autor}",
            "${commentprefixht} ${copy}",
            "${commentprefixht} ${licence}",
            "${commentendht}"
        ],
    }
}